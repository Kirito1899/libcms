{
  "env": {
    "browser": true
  },
  "globals": {
    "React": true,
    "$": true,
    "console": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "space-before-blocks": 2,
    "global-strict": [
      2,
      "always"
    ],
    "indent": [
      2,
      2,
      {
        "indentSwitchCase": true
      }
    ],
    "brace-style": 2,
    "space-after-keywords": 2,
    "strict": 2,
    "no-comma-dangle": 2,
    "no-unused-expressions": 2,
    "block-scoped-var": 2,
    "eol-last": 2,
    "dot-notation": 2,
    "consistent-return": 2,
    "no-unused-vars": [
      2,
      {
        "args": "none"
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "max-len": [
      1,
      80
    ],
    "no-alert": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "no-use-before-define": 0,
    "key-spacing": 0,
    "no-multi-spaces": 0
  }
}